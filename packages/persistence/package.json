    "pg": "7.12.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/lodash": "4.14.147",
    "@types/luxon": "1.15.2",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "@typescript-eslint/parser": "2.7.0",
    "eslint": "6.6.0",
    "eslint-plugin-jest": "23.0.3",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.5.0",
    "typescript": "3.7.2"
  }
